<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ibatorConfiguration
  PUBLIC "-//Apache Software Foundation//DTD Apache iBATIS Ibator Configuration 1.0//EN"
  "http://ibatis.apache.org/dtd/ibator-config_1_0.dtd">

  <ibatorConfiguration>
	<ibatorContext id="ibatorContext" targetRuntime="Ibatis2Java5" defaultModelType="flat">
		<ibatorPlugin type="org.apache.ibatis.ibator.plugins.SerializablePlugin"/>
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"  connectionURL="jdbc:mysql://localhost:3306/dietcrm" userId="dietcrm" 
			password="dietcrm">
    		</jdbcConnection>
		<javaModelGenerator targetPackage="com.zarcode.db.ibatis.vo.app"  targetProject="diet_crm_java\src">  
			<property name="enableSubPackages" value="false" />  
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<sqlMapGenerator targetPackage="com.zarcode.db.ibatis.maps.app"  targetProject="diet_crm_java\src">
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>
		<daoGenerator type="SPRING" targetPackage="com.zarcode.db.ibatis.dao.app" targetProject="diet_crm_java\src">  
			<property name="enableSubPackages" value="false" />  
			<property name="methodNameCalculator" value="extended" />
		</daoGenerator>
		<!-- question definition -->
		<table tableName="questionbase" domainObjectName="QuestionBase">
			<generatedKey column="quid" sqlStatement="MySQL" identity="true" />
		</table>
		<table tableName="questionoptions" domainObjectName="QuestionOptions">
			<generatedKey column="optId" sqlStatement="MySQL" identity="true" />
		</table>
		<table tableName="questioncolumns" domainObjectName="QuestionColumns">
			<generatedKey column="colId" sqlStatement="MySQL" identity="true" />
		</table>
		<table tableName="questionfolder" domainObjectName="QuestionFolder">
			<generatedKey column="folderId" sqlStatement="MySQL" identity="true" />
		</table>
		<table tableName="questionfoldermap" domainObjectName="QuestionFolderMap" />
		<!-- media definition -->
		<table tableName="mediaobject" domainObjectName="MediaObject">
			<generatedKey column="mediaId" sqlStatement="MySQL" identity="true" />
		</table>
		<table tableName="mediadata" domainObjectName="MediaData">
			<generatedKey column="dataId" sqlStatement="MySQL" identity="true" />
		</table>
		<table tableName="mediafolder" domainObjectName="MediaFolder">
			<generatedKey column="folderId" sqlStatement="MySQL" identity="true" />
		</table>
		<table tableName="mediafoldermap" domainObjectName="MediaFolderMap" />
		<!-- survey definition -->
		<table tableName="surveytemplate" domainObjectName="SurveyTemplate">
			<generatedKey column="surveyId" sqlStatement="MySQL" identity="true" />
		</table>
		<table tableName="installedsurvey" domainObjectName="InstalledSurvey" />
		<table tableName="participantgroup" domainObjectName="ParticipantGroup">
			<generatedKey column="groupId" sqlStatement="MySQL" identity="true" />
		</table>
		<table tableName="participant" domainObjectName="Participant">
			<generatedKey column="partId" sqlStatement="MySQL" identity="true" />
		</table>
		<table tableName="participantgroup" domainObjectName="ParticipantGroup">
			<generatedKey column="groupId" sqlStatement="MySQL" identity="true" />
		</table>
		<table tableName="participantgroupmap" domainObjectName="ParticipantGroupMap" />
	</ibatorContext>
</ibatorConfiguration>